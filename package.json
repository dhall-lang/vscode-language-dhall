{
    "name": "dhall-lang",
    "displayName": "Dhall Language Support",
    "description": "Syntax highlighting for the dhall programming language",
    "version": "0.0.4",
    "preview": true,
    "publisher": "panaeon",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "language",
        "dhall",
        "highlight",
        "syntax"
    ],
    "license": "MIT",
    "icon": "images/dhall-icon.png",
    "galleryBanner": {
        "color": "#293845",
        "theme": "dark"
    },
    "homepage": "https://github.com/PanAeon/vscode-language-dhall",
    "repository": {
        "type": "git",
        "url": "https://github.com/PanAeon/vscode-language-dhall.git"
    },
    "bugs": {
        "url": "https://github.com/PanAeon/vscode-language-dhall/issues"
    },
    "contributes": {
        "languages": [
            {
                "id": "dhall",
                "aliases": [
                    "Dhall",
                    "dhall"
                ],
                "extensions": [
                    "dhall"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dhall",
                "scopeName": "source.dhall",
                "path": "./syntaxes/dhall.tmLanguage.json"
            }
        ]
    },
    "comment": "oniguruma ~7.0.2",
    "devDependencies": {
        "@types/node": "^11.11.4",
        "ajv": "^6.10.0",
        "ts-node": "^8.0.2",
        "typescript": "^3.3.3",
        "vscode-tmgrammar-test": "^0.0.2"
    },
    "scripts": {
        "vscode:prepublish": "test -f ./syntaxes/dhall.tmLanguage.json",
        "prepare": "npx ts-node src/typescript/dhall-tmlanguage.ts > ./syntaxes/dhall.tmLanguage.json",
        "test": "npx vscode-tmgrammar-test -s source.dhall -g syntaxes/dhall.tmLanguage.json -t 'tests/**/*.test.dhall'"
    },
    "dependencies": {}
}
